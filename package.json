{
  "name": "i-d-test-case",
  "version": "1.0.0",
  "description": "i-d-test-case",
  "repository": {
    "type": "git",
    "url": "https://github.com/burakakkor/i-d-test-case"
  },
  "author": "Burak Akkor",
  "scripts": {
    "start": "tsc && node app.js",
    "test": "mocha --reporter spec --timeout 5000",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "report-cover": "cat ./coverage/lcov.info | coveralls",
    "watch": "gulp default",
    "watch-files": "gulp files",
    "tsc": "tsc",
    "pree2e": "webdriver-manager update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "pugtohtml": "pug --output-style compressed -o src/public/views/dist/ src/public/views/pug/*.pug"
  },
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "node-localstorage": "^1.3.0",
    "node-sass-middleware": "^0.10.0",
    "pug": "^2.0.0-beta6",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "supertest": "^2.0.1",
    "typescript": "~2.0.10",
    "should": "^11.2.0",
    "gulp": "^3.9.1",
    "gulp-pug": "^3.2.0",
    "gulp-mocha": "^3.0.1",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.15.1",
    "lodash": "^4.16.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "@types/node": "^6.0.46",
    "@types/jasmine": "^2.5.36"
  }
}
